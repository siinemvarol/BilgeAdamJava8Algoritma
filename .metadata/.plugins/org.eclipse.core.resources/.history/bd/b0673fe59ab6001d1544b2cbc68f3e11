package lesson015.veteriner;

public abstract class Insan {

	private String tc;
	private String name;
	
	public Insan(String tc, String name) {
		super();
		this.tc = tc;
		this.name = name;
	}

	public String getTc() {
		return tc;
	}

	public void setTc(String tcKimlik) {
		this.tc = tcKimlik;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
