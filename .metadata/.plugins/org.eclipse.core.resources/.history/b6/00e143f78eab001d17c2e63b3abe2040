package lesson008;

public class Question61 {
	
	private static boolean asalMi(int i) {
		boolean asalMi = true;
		for (int j = 2; j < i; j++) {
			if (i%j==0) {
				asalMi = false;
			}
		}
		return asalMi;
	}

	public static void main(String[] args) {
		// 1'den 1000'e kadar olan sayıların asal olanları bulan metodu yazalım
		// asalMi metonda sayılar asal mı diye kontrol edeceğiz
		// main metodda for döngüsüne sokup bulacağız
		
		for(int i = 1; i < 1000; i++) {
			System.out.println(asalMi(i));
		}

	}



}
