package homework2_24022023;

public class Takim {

	private String name;
	private int puan;
	private int kasa;
	private boolean iflasEtmeDurumu;
	
	public Takim (String name) {
		this.name = name;
	}	
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPuan() {
		return puan;
	}

	public void setPuan(int puan) {
		this.puan = puan;
	}

	public int getKasa() {
		return kasa;
	}

	public void setKasa(int kasa) {
		this.kasa = kasa;
	}

	public boolean isIflasEtmeDurumu() {
		return iflasEtmeDurumu;
	}

	public void setIflasEtmeDurumu(boolean iflasEtmeDurumu) {
		this.iflasEtmeDurumu = iflasEtmeDurumu;
	}
	
	

}
