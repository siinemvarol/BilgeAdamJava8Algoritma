package lesson015.veteriner;

public class Kedi extends Hayvan {
	
	private String tur;
	private String cins;
	private String karne;

	public Kedi(boolean kayitliMi, String dogumTarihi, String tur, String cins) {
		super(kayitliMi, dogumTarihi);
		this.tur = tur;
		this.cins = cins;
	}	

	public String getTur() {
		return tur;
	}

	public void setTur(String tur) {
		this.tur = tur;
	}

	public String getCins() {
		return cins;
	}

	public void setCins(String cins) {
		this.cins = cins;
	}

}
