package lesson015.veteriner;

public class Kopek extends Hayvan {

	private int asiSayisi;
	private String cins;
	private String tur;
	
	public Kopek(boolean kayitliMi, String dogumTarihi, int asiSayisi, String cins, String tur) {
		super(kayitliMi, dogumTarihi);
		this.asiSayisi = asiSayisi;
		this.cins = cins;
		this.tur = tur;
		
	}	

	public int getAsiSayisi() {
		return asiSayisi;
	}

	public void setAsiSayisi(int asiSayisi) {
		this.asiSayisi = asiSayisi;
	}

	public String getCins() {
		return cins;
	}

	public void setCins(String cins) {
		this.cins = cins;
	}

	public String getTur() {
		return tur;
	}

	public void setTur(String tur) {
		this.tur = tur;
	}

}
