package lesson018;

public enum ESikayet {

	YUKSEK("Apandist", 1),
	ORTA("Yanık", 2),
	DUSUK("Ağrı", 3);

	private String sikayet;
	private int oncelik;

	ESikayet(String sikayet, int oncelik) {
		this.sikayet = sikayet;
		this.oncelik = oncelik;

	}

	public String getSikayet() {
		return sikayet;
	}

	public int getOncelik() {
		return oncelik;
	}

}
