	// ve tekrarEtmeyenlerden çıkaralım
	
	// tekrarEdenler ve tekrarEtmeyenler yazdıralım
	
	// java
	// tekrarEdenler --> a
	// tekrarEtmeyenler --> j, v

	public static void main(String[] args) {
		HashSet<Character> tekrarEdenler = new HashSet<Character>();

	}

}
